{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-trailing-spaces": "error",
    "no-multiple-empty-lines": "error",
    "comma-dangle": "error",
    "indent": [
      "error",
      2
    ],
    "func-style": [
      "error",
      "expression", {
        "allowArrowFunctions": true
      }
    ]
  }
}
